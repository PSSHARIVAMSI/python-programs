"""

#11]Python Program for n-th Fibonacci number
"""
num=int(input())
a=0
b=1
if num==1:
    print(a)
elif num==2:
    print(a,b)
else:
    print(a,b,end=" ")
    for i in range (3,num+1): 
        c=a+b   
        a=b
        b=c
        print(c,end=" ")  
"""

#12]Python Program for printing Fibonacci series upto the n value?
"""
num=int(input())
a=0
b=1
i=1
c=1
while c<num:
    if i==1:
        print(a,end=" ")
    else:
        print(b,end=" ")
        c=a+b
        a=b
        b=c
    i+=1 

"""

#13]Python Program for How to check if a given number is Fibonacci number?
"""
num=int(input())
a=0
b=1
i=1
c=1
while c<=num:
  if i==1:
    print("yes")
    break
  else:
    #print(b,end=" ")
    c=a+b
    a=b
    b=c
    if(b==num):
      print("yes")
    else:
      print("No")
      break
    
  i+=1 

"""

#14]Python program to print all Prime numbers in an Interval
"""
start = int(input())
end = int(input())
  
for i in range(start, end+1):
  if i>1:
    for j in range(2,i):
        #print(i,j)
        if(i % j==0):
            break
    else:
        print(i)  
"""

#15]Python program to check whether a number is Prime or not
#SEIVES algorithm
"""
n=1000000
seive=[1 for i in range(n+1)]   #s=[0 0 1 1 0 1]
i=2
while (i*i<=n):      #9<=5
    if seive[i]==1:  #s[2]==1    
        for j in range(i*i,n+1,i): #4,6,2    4 
            seive[j]=0 #s[4]==0
    i=i+1

seive[0]=0
seive[1]=0

n=int(input()) #5

if seive[n]==1:   #s[5]==1
    print("Prime")
else:
    print("Not a prime")

"""

#16]Python program to find the COLLATZ sequence
#input-->7
#7 22 11 34 17 52 26 13 40 20 10 5 16 8 4 2 1 
"""
def hi(n):
    print(n,end=" ")
    if(n==1):
        return 0 
    elif(n%2==0):
        return(hi(n//2))
    else:
        return(hi((3*n)+1))
n=int(input())
hi(n)
